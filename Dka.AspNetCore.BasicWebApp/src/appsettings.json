{
  "Dka.AspNetCore.BasicWebApp": {
    "Logging": {
      "Console": {
        "LogLevel": {
          "Dka.AspNetCore.BasicWebApp.Startup": "Information",
          "Default": "Warning"
        }
      }
    },
    "Serilog": {
      "MinimumLevel": {
        "Default": "Warning",
        "Override": {
          "Dka.AspNetCore.BasicWebApp.Startup": "Information"
        }
      }
    },
    "ElasticConfiguration": {
      "Uri": "http://localhost:9200/",
      "Index": "basicwebapp-prod-{0:yyyy.MM.dd}",
      "LogLevel": {
        "Dka.AspNetCore.BasicWebApp.Startup": "Information",
        "Default": "Warning"
      }
    },
    "Unleash": {
      "AppName": "Dka.AspNetCore.BasicWebApp",
      "InstanceTag": "prod",
      "UnleashApi": "http://localhost:4242"
    },
    "webHost": {
      "urls:0": "http://localhost:5555",
      "urls:1": "https://localhost:5556"
    },
    "api": {
      "url": "https://localhost:6556"
    },
    "Jwt": {
      "Secret": "Very top secret phrase",
      "JwtBearerOptions": {
        "RequireHttpsMetadata": "false",
        "SaveToken": "true",
        "TokenValidationParameters": {
          "ValidateIssuerSigningKey": "true",
          "ValidateIssuer": "false",
          "ValidateAudience": "false"
        }
      }
    },
    "InternalApiClient": {
      "Password": "Very secret password"
    }
  }
}